# Any value specified here is post-pended after values specified by packages (eg by the package audit_userspace_auditctl)
linux_postpendToBuiltInCommandLine='vga=6'

# This prevents QEMU, syslinux, etc overriding the command line used to boot the kernel. Use with care.
linux_forceUseOfBuiltInCommandLine='false'

# Machine domainname (machine's hostname is the name of the folder, in this case 'example')
linux_domain='localdomain'

