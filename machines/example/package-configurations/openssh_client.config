libertine_openssh_client_Port='22'

# yes, no, ask
libertine_openssh_client_StrictHostKeyChecking='ask'

# yes, no, ask
libertine_openssh_client_UpdateHostKeys='no'

# yes, no, ask ; yes is quite powerful, as it then uses as SSHFP DNS record to verify the host key fingerprint, thus increasing the cost and complexity to an attacker
libertine_openssh_client_VerifyHostKeyDNS='no'
