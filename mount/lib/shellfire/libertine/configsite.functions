core_dependency_requires '*' rm mkdir cp
libertine_configsite_create()
{
	local forBuild="$1"
	
	local sysroot="$(libertine_public_inputSysrootPath)"

	local outputFilePath="$(libertine_public_inputSysrootPath)"/etc/config.site
	mkdir -m 0755 -p "$sysroot"/etc
	
	local rootPrefix
	_libertine_configsite_rootPrefix

	_libertine_configsite_createInvariant "$sysroot"
	_libertine_configsite_appendWithPrefix "$rootPrefix" "$package"
	_libertine_configsite_append "$sysroot" "$forBuild" "$sysroot" "$(libertine_public_buildNeedsPath)"
	
	# In case a package ships this in error
	rm -rf config.cache
	cp "$outputFilePath" config.cache
}

_libertine_configsite_rootPrefix()
{
	if $forBuild; then
		rootPrefix="$(libertine_public_outputBuildSysrootPath)"
	else
		rootPrefix=''
	fi
}

libertine_configsite_createForInitramfsSysroot()
{
	local forBuild=false
	
	local sysroot="$(libertine_public_inputSysrootPath)"
	
	local rootPrefix
	_libertine_configsite_rootPrefix

	_libertine_configsite_createInvariant "$sysroot"
	_libertine_configsite_appendWithPrefix "$rootPrefix"
	_libertine_configsite_append "$sysroot" "$forBuild" '/' '/usr/bin'
}

core_dependency_requires '*' cat
_libertine_configsite_appendWithPrefix()
{
	local rootPrefix="$1"
	local package="$2"
	
	cat >>"$outputFilePath" <<-EOF
		prefix=${rootPrefix}/usr
		exec_prefix=${rootPrefix}/usr
		bindir=${rootPrefix}/usr/bin
		sbindir=${rootPrefix}/usr/sbin
		libexecdir=${rootPrefix}/usr/libexec
		sysconfdir=${rootPrefix}/etc
		sharedstatedir=${rootPrefix}/com
		localstatedir=${rootPrefix}/var
		libdir=${rootPrefix}/usr/lib
		includedir=${rootPrefix}/usr/include
		oldincludedir=${rootPrefix}/usr/include
		datarootdir=${rootPrefix}/usr/share
		datadir=${rootPrefix}/usr/share
		infodir=${rootPrefix}/usr/share/info
		localedir=${rootPrefix}/usr/share/locale
		mandir=${rootPrefix}/usr/share/man
		docdir=${rootPrefix}/usr/share/doc/${package}
		htmldir=${rootPrefix}/usr/share/doc/${package}/html
		dvidir=${rootPrefix}/usr/share/doc/${package}/dvi
		pdfdir=${rootPrefix}/usr/share/doc/${package}/pdf
		psdir=${rootPrefix}/usr/share/doc/${package}/ps
	EOF
}

core_dependency_requires '*' cat
_libertine_configsite_append()
{
	local sysroot="$1"
	local forBuild="$2"
	local actualSysroot="$3"
	local path="$4"
	
	if core_variable_isSet libertine_public_autoconf_removeCFlags; then
		# Quotes deliberately ommitted
		IFS=' ' set -- $libertine_public_autoconf_removeCFlags
	else
		set --
	fi
	
	if $forBuild; then
		if $libertine_buildIsBigEndian; then
			local isBigEndian=yes
		else
			local isBigEndian=no
		fi
		local target="$libertine_build"
		local targetAutoconf="$libertine_buildAutoconf"
		local cFlags="$(libertine_public_buildCCompilerFlags "$@")"
		local cxxFlags="$(libertine_public_buildCxxCompilerFlags "$@")"
	else
		if $libertine_hostIsBigEndian; then
			local isBigEndian=yes
		else
			local isBigEndian=no
		fi
		local target="$libertine_host"
		local targetAutoconf="$libertine_hostAutoconf"
		local cFlags="$(libertine_public_hostCCompilerFlags "$@")"
		local cxxFlags="$(libertine_public_hostCxxCompilerFlags "$@")"
	fi
	
	if [ "$libertine_cmakeCompilerId" = 'GNU' ]; then
		local isGccCompiler=yes
		local isGccLinker=yes
		local isGccAssembler=yes
	else
		local isGccCompiler=no
		local isGccLinker=yes
		local isGccAssembler=yes
	fi
	
	cat >>"$outputFilePath" <<-EOF
		ac_cv_build=${libertine_buildAutoconf}
		ac_cv_c_bigendian=${isBigEndian}
		ac_cv_c_compiler_gnu=${isGccCompiler}
		ac_cv_cxx_compiler_gnu=${isGccCompiler}
		ac_cv_env_build_alias_value=${libertine_build}
		ac_cv_env_host_alias_value=${target}
		with_gas=${isGccAssembler}
		with_sysroot=${sysroot}
		lt_cv_truncate_bin='${path}/dd bs=4096 count=1'
		lt_cv_prog_gnu_ld=${isGccLinker}
		lt_cv_path_LD=${path}/${target}-ld
		lt_cv_path_NM='${path}/${target}-nm -B'
		ac_cv_host=${targetAutoconf}
		ac_cv_target=${targetAutoconf}
		ac_cv_path_BZIP2='${path}/bzip2'
		ac_cv_path_CAT='${path}/cat'
		ac_cv_path_CXX='${path}/c++'
		ac_cv_path_EGREP='${path}/grep -E'
		ac_cv_path_EXPR='${path}/expr'
		ac_cv_path_FGREP='${path}/grep -F'
		ac_cv_path_GREP=${path}/grep
		ac_cv_path_GZIP='${path}/gzip'
		ac_cv_path_INSTALL='${path}/install -c'
		ac_cv_path_KILL='${path}/kill'
		ac_cv_path_LOGIN_PROGRAM_FALLBACK='${path}/login'
		ac_cv_path_MV='${path}/mv'
		ac_cv_path_PATH_GROUPADD_PROG='${path}/groupadd'
		ac_cv_path_PATH_PASSWD_PROG='${path}/passwd'
		ac_cv_path_PATH_USERADD_PROG='${path}/useradd'
		ac_cv_path_RM='${path}/rm'
		ac_cv_path_SED='${path}/sed'
		ac_cv_path_SH='${path}/sh'
		ac_cv_path_TAR='${path}/tar'
		ac_cv_path_TELNET='${path}/telnet'
		ac_cv_path_UNCOMPRESS='${path}/gunzip'
		ac_cv_path_UNZIP='${path}/unzip'
		ac_cv_path_ZCAT='${path}/zcat'
		ac_cv_path_install='${path}/install -c'

		ac_cv_path_lt_DD='${path}/dd'
		ac_cv_path_mkdir='${path}/mkdir'

		ac_cv_prog_AR=${target}-ar
		ac_cv_prog_OBJDUMP=${target}-objdump
		ac_cv_prog_RANLIB=${target}-ranlib
		ac_cv_prog_STRIP=${target}-strip
	
		ac_cv_prog_CC='${target}-cc ${cFlags} --sysroot=${sysroot}'
		ac_cv_prog_CPP='${target}-cc ${cFlags} --sysroot=${sysroot} -E'
		ac_cv_prog_CXX='${target}-c++ ${cxxFlags} --sysroot=${sysroot}'
	EOF
}

